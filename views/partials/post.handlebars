<a href="/api/posts/{{post.id}}">
<div class="container grey darken-4">
    <div class="row">
        <div class="row center">
            <div class="col s4">

                <img class="z-depth-3 responsive-img" src="post.image_key" alt="post.alt_text" />
                <h5>{{post.title}}</h5>
                <h5>{{post.creator-id}}</h5>

            </div>
        </div>
    </div>
</div>
</a>
